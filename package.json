{
  "name": "arthack-proxy",
  "version": "0.1.0",
  "author": "Mike Bannister <notimpossiblemike@gmail.com>",
  "license": "MIT",
  "description": "ArtHack Proxy",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build:watch": "nodemon --watch src/ --ext ts --exec tsup",
    "dev": "nodemon --watch dist --exec node dist/index.js",
    "start": "node dist/index.js",
    "format": "prettier --write \"**/*.{js,mjs,jsx,ts,tsx,json,md,mdx,css,yml,yaml}\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.19.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
